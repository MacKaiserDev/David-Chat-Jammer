<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.1" />
  </startup>
  <appSettings>
    <add key="ConnectionString" value="Data Source=localhost\Test;Initial Catalog=David TeamBoards Database;Persist Security Info=True;User ID=sa;Password=Admin123!" />
    <!-- ConnectionString: Gibt den Weg zum MSSQL Server an.  -->
    <add key="WaitTime" value="4000" />
    <!-- WaitTime: Gibt die Zeit zwischen 2 Durchläufen in Minisekunden an -->
    <add key="Message" value="Nachricht entfernt!" />
    <!-- Message: Gibt die Zeichenfolge an, durch die die Chatnachrichten ausgetauscht werden sollen.  -->
    <add key="Debug" value="false" />
    <!-- Debug True aktiviert die Debugausgabe(Aktuell nicht vollständig implementiert) -->
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.SqlServer.SqlClrProvider" publicKeyToken="89845dcd8080cc91" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-15.100.0.0" newVersion="15.100.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>